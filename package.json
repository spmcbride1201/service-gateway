{
  "name": "service-gateway",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "watch": {
    "buildAndServe": {
      "patterns": [
        "templates",
        "properties"
      ],
      "extensions": "mustache,json"
    }
  },
  "scripts": {
    "start": "npm-watch",
    "build": "./node_modules/mustache/bin/mustache ./properties/gatekeeperMock.json ./templates/html.mustache > ./index.html",
    "serve": "./node_modules/serve/bin/serve.js .",
    "buildAndServe": "npm run build && npm run serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spmcbride1201/service-gateway.git"
  },
  "keywords": [],
