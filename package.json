  "bugs": {
    "url": "https://github.com/spmcbride1201/service-gateway/issues"
  },
  "homepage": "https://github.com/spmcbride1201/service-gateway#readme",
  "dependencies": {
    "mustache": "^2.3.0",
    "npm-watch": "^0.1.9",
    "serve": "^5.1.5"
  }
}
